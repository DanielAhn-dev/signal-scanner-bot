{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": null,
    "buildCommand": null,
    "devCommand": null,
    "installCommand": null,
    "functions": {
        "api/telegram.ts": {
            "maxDuration": 10
        },
        "api/worker.ts": {
            "maxDuration": 10
        }
    },
    "outputDirectory": ".",
    "crons": [
        {
            "path": "/api/worker?token=${CRON_SECRET}",
            "schedule": "30 8 * * 1-5"
        }
    ]
}