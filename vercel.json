{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "tsc",
    "outputDirectory": "dist",
    "regions": [
        "icn1"
    ],
    "crons": [
        {
            "path": "/api/cron/briefing",
            "schedule": "30 8 * * 1-5"
        },
        {
            "path": "/api/cron/close",
            "schedule": "40 15 * * 1-5"
        }
    ]
}