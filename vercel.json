{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": null,
    "buildCommand": null,
    "devCommand": null,
    "installCommand": null,
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 10
        }
    },
    "outputDirectory": ".",
    "crons": [
        {
            "path": "/api/cron/briefing",
            "schedule": "30 23 * * *"
        },
        {
            "path": "/api/cron/close",
            "schedule": "40 6 * * *"
        }
    ]
}