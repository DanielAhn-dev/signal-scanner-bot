{
  "name": "signal-scanner-bot",
  "version": "1.0.2",
  "description": "Telegram Stock Signal Scanner Bot with Vercel Functions",
  "private": true,
  "scripts": {
    "local": "vercel dev",
    "build": "tsc --noEmit",
    "deploy": "vercel --prod",
    "sync:sectors": "tsx scripts/syncSectors.ts",
    "sync:sectorstostocks": "tsx scripts/syncSectorsToStocks.ts",
    "seed:sectors": "tsx scripts/seedSectors.ts",
    "fix:stocks": "tsx scripts/fixStocksAndMapSectors.ts",
    "gen:sectors": "tsx scripts/generateSectorsJson.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vercel": "^33.0.0"
  }
}